generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id String @id @default(uuid())
  name String
  email String @unique

  @@map("users")
}

model Transcripts {
  id String @id @default(uuid())
  title String @default("recording")
  transcript String
  duration String
  date DateTime @default(now())
  client_audio_id String @unique
  userEmail String

  @@map("transcripts")
}